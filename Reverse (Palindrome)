import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        String A=sc.next();
        StringBuilder obj = new StringBuilder(A);
        System.out.println(A.equalsIgnoreCase(obj.reverse().toString())? "Yes" : "No");
        
        
    }
}
